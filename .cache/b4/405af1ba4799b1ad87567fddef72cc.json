us: 8px;\n            margin-bottom: 18px;\n\n            img {\n                padding-right: 5px;\n            }\n\n            span {\n                padding-left: 25px;\n                vertical-align: top;\n            }\n\n            &:nth-child(2) {\n                margin-left: 50px;\n            }\n\n            &:nth-child(3) {\n                margin-left: 95px;\n            }\n        }\n    }\n\n    p {\n        color: hsl(303, 10%, 53%);\n        line-height: 26px;\n        margin-top: 14px;\n        letter-spacing: 0.1px;\n        font-size: 14px;\n    }\n\n    &.reviews {\n        display: grid;\n        grid-template-columns: 1fr 1fr 1fr;\n        grid-gap: 1.9rem;\n        margin-top: 68px;\n\n        .buyer {\n            display: flex;\n            margin-top: 22px;\n        }\n\n        .buyer-info {\n            padding-top: 4px;\n            padding-left: 23px;\n        }\n\n        h3 {\n            color: $white;\n            font-size: 13px;\n            padding-bottom: 4px;\n        }\n\n        span {\n            color: $soft-pink;\n            font-size: 13px;\n        }\n\n        p {\n            padding: 11px 3px;\n            color: $white;\n            line-height: 23px;\n            font-size: 12px;\n        }\n\n        img {\n            width: 40px;\n            border-radius: 100%;\n        }\n\n        article {\n            background-color: $very-dark-magenta;\n            height: 234px;\n            border-radius: 8px;\n            padding: 18px 32px;\n\n            &:nth-child(2) {\n                margin-top: 16px;\n            }\n\n            &:nth-child(3) {\n                margin-top: 32px;\n            }\n        }\n    }\n}\n\n@media screen and (max-width: 900px) {\n    main {\n        &.container {\n            padding-top: 90px;\n        }\n    }\n\n    section {\n        text-align: center;\n\n        &.header {\n            display: grid;\n            grid-template-columns: 1fr;\n            grid-template-rows: 1fr 1fr;\n        }\n\n        .intro {\n            margin: auto;\n            min-width: 89%;\n        }\n\n        h1 {\n            font-size: 30px;\n            line-height: normal;\n        }\n\n        p {}\n\n        .rating {\n            margin-top: -15px;\n\n            .line {\n                display: flex;\n                flex-flow: wrap;\n                justify-content: center;\n                margin: 10px auto;\n                width: 86%;\n\n                span {\n                    padding-top: 10px;\n                }\n\n                &:nth-child(2) {\n                    margin: 10px auto;\n                }\n\n                &:nth-child(3) {\n                    margin: 10px auto;\n                }\n            }\n        }\n\n        &.reviews {\n            grid-template-columns: none;\n            grid-template-rows: 1fr 1fr 1fr;\n            grid-gap: 1rem;\n            margin: 66px 24px;\n\n            article {\n                height: 248px;\n                text-align: left;\n\n                &:nth-child(2) {\n                    margin-top: 0;\n                }\n\n                &:nth-child(3) {\n                    margin-top: 0;\n                }\n            }\n        }\n\n    }\n}"},"lineCount":null}},"error":null,"hash":"f2015bfcba536ddd5b8621583a3390e6","cacheData":{"env":{}}}